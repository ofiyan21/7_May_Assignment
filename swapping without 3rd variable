import java.util.Scanner;
public class swap_program
{
      
    public static void main(String args[])
    {
       
        swapping_numbers();
    }
    public  static void swapping_numbers()
    {
      int num1,num2;
      Scanner sc=new Scanner(System.in);
      System.out.println("Enter two numbers: ");
      num1=sc.nextInt();
      num2=sc.nextInt();
      
      num1=num1+num2;
      num2=num1-num2;
      num1=num1-num2;
      System.out.println("the values of num1= " +num1+ " the values of num2=" +num2);
    }
}

