import java.util.Scanner;
public class factorial 
{
   public static void main(String args[])
   {
      int fact=1;
      int i=1;
      int num;
   Scanner sc=new Scanner(System.in);
   System.out.println("Enter a number");
   num=sc.nextInt();
   
   while(i<=num)
   {
       fact=fact*i;
       i++;
   }
   System.out.println("\nFactorial of " + num + " is: "+fact);
   }   

}    
